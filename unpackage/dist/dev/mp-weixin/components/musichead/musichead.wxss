
.musichead {
	width: 100%;
	height: 75px;
	font-size: 16px;
	line-height: 80px;
	text-align: center;
	color: black;
	position: relative;
	font-weight: bolder;
}
.music-head-icon {
	position: absolute;
	left: 8px;
	top: 50%;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
	width: 90px;
	height: 31px;
	background: rgba(0, 0, 0, .2);
	color: white;
	line-height: 31px;
	border-radius: 15px;
	display: flex;
	justify-content: space-evenly;
}

